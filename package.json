{
  "name": "knuckle",
  "version": "0.2.2",
  "description": "Front end script",
  "license": "MIT",
  "author": "",
  "homepage": "https://github.com/GuillaumeAmat/knuckle#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GuillaumeAmat/knuckle.git"
  },
  "bugs": {
    "url": "https://github.com/GuillaumeAmat/knuckle/issues"
  },
  "keywords": [
    "script",
    "lint",
    "format",
    "test"
  ],
  "engines": {
    "node": ">=8.9.1"
  },
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": {
    "knuckle": "dist/bin/index.js"
  },
  "scripts": {
    "knuckle": "babel-node src/bin/index.js",
    "format": "npm run knuckle -- prettier --write 'src/**/*.{js,json}'",
    "format:test": "npm run knuckle -- prettier --list-different 'src/**/*.{js,json}'",
    "lint-js": "npm run knuckle -- eslint src",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "echo TODO",
    "build": "babel src -d dist",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "preversion": "npm test && npm run build",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-latest": "^6.24.1",
    "cross-spawn": "^6.0.5",
    "husky": "^0.14.3"
  },
  "dependencies": {
    "@commitlint/cli": "^7.1.1",
    "@commitlint/config-conventional": "^7.1.1",
    "all-contributors-cli": "^5.4.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "commander": "^2.17.1",
    "cosmiconfig": "^5.0.6",
    "eslint": "^5.5.0",
    "eslint-config-last": "0.0.5",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
    "lint-staged": "^7.2.2",
    "lodash": "^4.17.5",
    "prettier": "^1.14.2",
    "read-pkg-up": "^4.0.0",
    "tmp": "0.0.33"
  }
}
